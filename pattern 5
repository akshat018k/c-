#include <stdio.h>
#include <conio.h>

int main() {
    int i, j, k;
    int n = 5;
    clrscr();
    for (i = 1; i <= n; i++) {
        for (j = i; j < n; j++) {
            printf("  ");
        }

        for (j = n; j >= n - i + 1; j--) {
            printf("%d ", j);
        }
        
        for (k = n - i + 2; k <= n; k++) {
            printf("%d ", k);
        }
        printf("\n");
    }
    getch();
}


"""
        5 
      5 4 5 
    5 4 3 4 5 
  5 4 3 2 3 4 5 
5 4 3 2 1 2 3 4 5 

"""
